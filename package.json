{
  "name": "anibook-backend",
  "version": "2.0.0",
  "description": "Backend para a aplicação Anibook",
  "main": "index.js",
  "author": {
    "name": "Eduardo Henrique",
    "email": "edu.ed322@gmail.com",
    "url": "https://github.com/Bruce2107"
